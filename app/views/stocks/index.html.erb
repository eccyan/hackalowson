<h1>Listing stocks</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Storeid</th>
      <th>Odenid</th>
      <th>Number</th>
      <th>Available</th>
      <th>Entrytime</th>
      <th>Recordedtime</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @stocks.each do |stock| %>
      <tr>
        <td><%= stock.id %></td>
        <td><%= stock.storeId %></td>
        <td><%= stock.odenId %></td>
        <td><%= stock.number %></td>
        <td><%= stock.available %></td>
        <td><%= stock.entryTime %></td>
        <td><%= stock.recordedTime %></td>
        <td><%= link_to 'Show', stock %></td>
        <td><%= link_to 'Edit', edit_stock_path(stock) %></td>
        <td><%= link_to 'Destroy', stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stock', new_stock_path %>
